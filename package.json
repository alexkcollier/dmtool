{
  "name": "dmtool",
  "version": "2.28.7",
  "description": "D&D 5e Utilities",
  "author": "Alex Collier <alexkcollier@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "corp-release": "corp-semantic-release",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint '**/*.vue' '**/*.scss'",
    "lint": "npm run lint:js && npm run lint:style",
    "lintfix:js": "npm run lint:js -- --fix",
    "lintfix:style": "npm run lint:style -- --fix",
    "lintfix": "npm run lintfix:js && npm run lintfix:style"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:js",
      "git add"
    ],
    "*.vue": [
      "npm run lint:js",
      "npm run lint:style",
      "git add"
    ],
    "*.scss": [
      "npm run lint:style",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.0.0-beta.19",
    "buefy": "^0.8.10",
    "dice-roller-dnd": "^1.0.7",
    "fuse.js": "^3.4.6",
    "hyperid": "^2.0.3",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.orderby": "^4.6.0",
    "lodash.throttle": "^4.1.1",
    "nuxt": "^2.11.0",
    "vue-scrollto": "^2.17.1",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@acollier/eslint-config-vue": "^1.4.0",
    "@acollier/stylelint-config-scss": "^2.1.2",
    "babel-eslint": "^10.0.3",
    "corp-semantic-release": "^7.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "fibers": "^4.0.2",
    "husky": "^4.2.1",
    "lint-staged": "^10.0.7",
    "prettier": "1.14.3",
    "sass": "^1.25.0",
    "sass-loader": "^8.0.2",
    "stylelint": "^10.1.0"
  }
}

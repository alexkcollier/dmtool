{
  "name": "dmtool",
  "version": "2.28.8",
  "description": "D&D 5e Utilities",
  "author": "Alex Collier <alexkcollier@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "corp-release": "corp-semantic-release",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint '**/*.vue' '**/*.scss' --allow-empty-input",
    "lint": "npm run lint:js && npm run lint:style",
    "lintfix:js": "npm run lint:js -- --fix",
    "lintfix:style": "npm run lint:style -- --fix",
    "lintfix": "npm run lintfix:js && npm run lintfix:style"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:js",
      "git add"
    ],
    "*.vue": [
      "yarn lint:js",
      "yarn lint:style",
      "git add"
    ],
    "*.scss": [
      "yarn lint:style",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.3.5",
    "buefy": "^0.9.4",
    "dice-roller-dnd": "^1.0.7",
    "fuse.js": "^6.4.6",
    "hyperid": "^2.1.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.orderby": "^4.6.0",
    "lodash.throttle": "^4.1.1",
    "nuxt": "^2.15.2",
    "vue-scrollto": "^2.20.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@acollier/eslint-config": "2.1.0",
    "@acollier/eslint-config-vue": "1.4.0",
    "@acollier/stylelint-config-scss": "^2.1.2",
    "babel-eslint": "^10.1.0",
    "corp-semantic-release": "^7.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": ">=7.6.0",
    "fibers": "^5.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.11.0"
  }
}
